script/source = "extends Node


onready var data : Data = $\"../../Data\"


func _ready() -> void:
	data.show_camera.current = true
	data.is_on_show = true
	queue_free()
	
"

[sub_resource type="GDScript" id=3]
script/source = "extends Node


const SPEED : float = -50.0

onready var data : Data = $\"../../Data\"


func _process(delta : float) -> void:
	if data.is_on_show:
		data.show_node.rotate_y(deg2rad(delta * SPEED))
"

[sub_resource type="GDScript" id=7]
script/source = "extends Node


var time : float = 0
var initial_show_cam_trans : Transform

onready var data : Data = $\"../../Data\"


func _process(_delta : float) -> void:
	if not data.is_on_play_initiation:
		return
	
	data.show_camera.fov = lerp(70, 120, time)
	
	if not $Tween.is_active():
		data.is_on_play_initiation = false


func _input(event : InputEvent) -> void:
	if data.is_on_show:
		if event.is_action_pressed(\"start\"):
			data.is_on_show = false
			data.is_on_play_initiation = true
			start_initiation()


func start_initiation() -> void:
	initial_show_cam_trans = data.show_camera.transform
	$Tween.interpolate_property(self, \"time\", 0, 1, 2, Tween.TRANS_LINEAR, Tween.EASE_IN)
	$Tween.start()

"

